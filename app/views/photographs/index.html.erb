<div> <h2 class="page-header">活动作品管理 </h2>
</div>
<div>
  <div class="">
    <%=form_tag search_activity_photographs_path(@activity), method:"get", class:"form-inline", role:"form" do%>
      <div class="form-group">
        <label for="name">作品名称</label>
        <%=text_field_tag "name", '', class:"form-control"%>
      </div>
      <div class="form-group">
        <%=submit_tag "查询",  class:"form-control"%>
      </div>
    <%end%>
  </div>
  <div style="padding-top:20px;">
    <%= render partial: "records" %>
  </div>
</div>
